								{ image: { normal: "SP_ChamferLT_Sm_N.png", dark_normal: "SP_ChamferLT_Sm_N_D.png" }, show_border: true, value: 3, alt: "$$$/ShapeCoreUI/RectangleProperties/ToolTip/ChamferCornerType=Chamfer"}
							],
					wants_focus: false);
